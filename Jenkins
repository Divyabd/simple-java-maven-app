pipeline{
 agent {
    docker {
      image 'maven:3.6.3-jdk-11'
      args '-v /root/.m2:/root/.m2'
    }
  }

  stages {
   
  
   
    
  
stage('Sonar Analysis'){
            steps{
                
                    withSonarQubeEnv('sonartwo'){
                        withMaven(maven:'maven'){
                            sh 'mvn sonar:sonar'
                        }
                        
                  }
                }
            
            
        }
  
  
  }
}
