pipeline{
      agent any 
          stages{
                stage('Build'){
                             steps{
                                    echo 'hii'
                                  }
  
                             }
          stage('Test') {
                     steps{
                          sh 'mvn -B -DskipTests clean package'
                          }
                      post{
                           always {
                                 echo 'hello'
   
                                    }
                            }
                        }
                   }
         }
